{
  "name": "joydeep-bhowmik/laravel-point-vault",
  "description": "A Laravel point management system to manage point transactions within Eloquent models",
  "type": "library",
  "version": "v1.0.0",
  "license": "MIT",
  "keywords": [
    "laravel",
    "laravel point management system",
    "laravel point vault"
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "JoydeepBhowmik\\LaravelPointVault\\": "src/",
      "JoydeepBhowmik\\LaravelPointVault\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "joydeep-bhowmik",
      "email": "bhowmikj9432@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^9.4",
    "pestphp/pest": "^3.2",
    "pestphp/pest-plugin-laravel": "^3.0",
    "illuminate/database": "*",
    "fakerphp/faker": "^1.23"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
